    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-prefix-selector": "^1.16.0",
