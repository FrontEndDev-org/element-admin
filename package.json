{
  "name": "element-admin",
  "version": "1.0.0",
  "description": "基于 Element Plus 封装的中后台场景组件库",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/element-admin.umd.cjs",
  "module": "./dist/element-admin.js",
  "exports": {
    ".": {
      "import": "./dist/element-admin.js",
      "require": "./dist/element-admin.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "play": "vite",
    "build": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "prepare": "husky install",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:coverage": "vitest run --coverage"
